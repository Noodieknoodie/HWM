{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/rest/*",
            "/data-api/*",
            "/.auth/*"
        ]
    },
    "mimeTypes": {
        ".json": "application/json"
    },
    "globalHeaders": {
        "Content-Security-Policy": "frame-ancestors 'self' https://teams.microsoft.com https://*.teams.microsoft.com https://*.teams.microsoft.us https://*.teams.microsoft.cn https://*.office.com"
    },
    "routes": [
        {
            "route": "/data-api/*",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ],
    "auth": {
        "identityProviders": {
            "azureActiveDirectory": {
                "registration": {
                    "openIdIssuer": "https://login.microsoftonline.com/e621abc4-3baa-4b93-badc-3b99e8609963",
                    "clientIdSettingName": "AAD_CLIENT_ID"
                },
                "login": {
                    "loginParameters": "scope=openid profile email"
                }
            }
        }
    }
}